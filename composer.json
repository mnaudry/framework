{
    "name": "mnaudry/bomoyi",
    "description": "Doing a framewrok using symfony for fun",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Bomoyi\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "symfony/http-foundation": "^6.2",
        "symfony/routing": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/event-dispatcher": "^6.2"
    }
}
